<?xml version="1.0" encoding="UTF-8"?>
<plugin xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android" id="cordova-plugin-MediaExporter" version="0.0.1">
    <name>MediaExporter</name>
    <js-module name="MediaExporter" src="www/MediaExporter.js">
        <clobbers target="MediaExporter" />
    </js-module>
    <platform name="ios">
        <config-file parent="/*" target="config.xml">
            <feature name="MediaExporter">
                <param name="ios-package" value="MediaExporter" />
            </feature>
        </config-file>
        <source-file src="src/ios/MediaExporter.m" />
        <header-file src="src/ios/MediaExporter.h" />
    </platform>
</plugin>
